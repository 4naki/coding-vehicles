            holoModel(I+1,"")
            holoAlpha(I+1,0)
        }else{
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_"+Text3:explode("")[I2,string]+".mdl")
        }
        }
        holoBodygroup(I+1,0,2)
        holoMaterial(I+1,"models/debug/debugwhite")
        holoColor(I+1,vec(255))
    }
}

