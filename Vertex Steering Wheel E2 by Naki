@name Vertex Steering Wheel E2 by Naki
@inputs [Base WheelPlate]:entity
@persist Int
if(first()){
    
    entity():setAlpha(0)
    
    holoCreate(1)
    holoPos(1,entity():toWorld(vec(0,0,2)))
    holoAng(1,entity():toWorld(ang(-90,0,0)))
    holoModel(1,"models/sprops/trans/stwheels/stwheel_1.mdl")
    holoMaterial(1,"models/debug/debugwhite")
    holoColor(1,vec(28))
    holoSkin(1,2)
    holoParent(1,entity())
    
    holoCreate(2)
    holoPos(2,entity():toWorld(vec(0,0,-5)))
    holoAng(2,entity():toWorld(ang(0,0,0)))
    holoModel(2,"models/sprops/cylinders/size_3/cylinder_6x12.mdl")
    holoMaterial(2,"models/debug/debugwhite")
    holoColor(2,vec(55))
    
    holoParent(2,holoEntity(1))
    holoScale(2,vec(0.65,0.65,1))
    
    holoCreate(3)
    holoPos(3,entity():toWorld(vec(0,0,3)))
    holoAng(3,entity():toWorld(ang(0,0,90)))
    holoModel(3,"models/sprops/misc/tubes_superthin/size_1/tube_0_5.mdl")
    holoMaterial(3,"models/debug/debugwhite")
    holoColor(3,vec(255))
    
    holoParent(3,holoEntity(1))
    holoScale(3,vec(0.3,1,0.3))
    
    Scale=vec(0.28)
    
    Text1="vertex"
    Text2="japan"
    Text3="@_Tande_co.,ltd"
    
    Int=250
    
    timer("toptext",Int)
    for(I2=1,Text1:explode(""):count()){
        I=I2+100
        holoCreate(I+1)
        if(I2==1){
            holoPos(I+1,entity():toWorld((vec(0,-0.75*Text1:explode(""):count(),3.25)+vec(0,0,0))*vec(Scale:x(),Scale:y(),1)))
        }else{
            holoPos(I+1,holoEntity(I):toWorld(vec(-1.8,0,0)*vec(Scale:x(),Scale:y(),Scale:z())))
        }
        holoAng(I+1,entity():toWorld(ang(0,-90,90)))
        holoParent(I+1,holoEntity(1))
        holoScale(I+1,vec(0.12,0.05,0.13)*vec(Scale:y(),Scale:x(),Scale:z()))
        
        if(Text1:explode("")[I2,string]=="["){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_lbracket.mdl")
        }elseif(Text1:explode("")[I2,string]=="]"){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_rbracket.mdl")
        }elseif(Text1:explode("")[I2,string]=="."){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_prd.mdl")
        }elseif(Text1:explode("")[I2,string]==","){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_com.mdl")
        }elseif(Text1:explode("")[I2,string]=="-"){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_min.mdl")
        }elseif(Text1:explode("")[I2,string]=="_"){
            holoModel(I+1,"")
            holoAlpha(I+1,0)
        }else{
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_"+Text1:explode("")[I2,string]+".mdl")
        }
        holoBodygroup(I+1,0,2)
        holoMaterial(I+1,"models/debug/debugwhite")
        holoColor(I+1,vec(255))
    }
    
    for(I2=1,Text2:explode(""):count()){
        I=I2+200
        holoCreate(I+1)
        if(I2==1){
            holoPos(I+1,entity():toWorld((vec(0,-0.3*Text2:explode(""):count(),3.25)+vec(1.5,0,0))*vec(Scale:x(),Scale:y(),1)))
        }else{
            holoPos(I+1,holoEntity(I):toWorld(vec(-1.52,0,0)*vec(Scale:x(),Scale:y(),Scale:z())))
        }
        holoAng(I+1,entity():toWorld(ang(0,-90,90)))
        holoParent(I+1,holoEntity(1))
        holoScale(I+1,vec(0.12*0.35,0.05,0.13*0.4)*vec(Scale:y(),Scale:x(),Scale:z()))
        
        if(Text2:explode("")[I2,string]=="["){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_lbracket.mdl")
        }elseif(Text2:explode("")[I2,string]=="]"){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_rbracket.mdl")
        }elseif(Text2:explode("")[I2,string]=="."){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_prd.mdl")
        }elseif(Text2:explode("")[I2,string]==","){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_com.mdl")
        }elseif(Text2:explode("")[I2,string]=="-"){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_min.mdl")
        }elseif(Text2:explode("")[I2,string]=="_"){
            holoModel(I+1,"")
            holoAlpha(I+1,0)
        }else{
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_"+Text2:explode("")[I2,string]+".mdl")
        }
        holoBodygroup(I+1,0,2)
        holoMaterial(I+1,"models/debug/debugwhite")
        holoColor(I+1,vec(255))
    }
    
    for(I2=1,Text3:explode(""):count()){
        I=I2+300
        holoCreate(I+1)
        if(I2==1){
            holoPos(I+1,entity():toWorld((vec(0,-0.2*Text3:explode(""):count(),3.25)+vec(3.4,0,0))*vec(Scale:x(),Scale:y(),1)))
        }else{
            holoPos(I+1,holoEntity(I):toWorld(vec(-0.45,0,0)*vec(Scale:x(),Scale:y(),Scale:z())))
        }
        holoAng(I+1,entity():toWorld(ang(0,-90,90)))
        holoParent(I+1,holoEntity(1))
        
        if(Text3:explode("")[I2,string]=="@"){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_atsign.mdl")
            holoScale(I+1,vec(0.12*0.7,0.05,0.13*0.7)*vec(Scale:y(),Scale:x(),Scale:z()))
        }else{
            holoScale(I+1,vec(0.12*0.3,0.05,0.13*0.4)*vec(Scale:y(),Scale:x(),Scale:z()))
        if(Text3:explode("")[I2,string]=="["){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_lbracket.mdl")
        }elseif(Text3:explode("")[I2,string]=="]"){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_rbracket.mdl")
        }elseif(Text3:explode("")[I2,string]=="."){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_prd.mdl")
        }elseif(Text3:explode("")[I2,string]==","){
            holoModel(I+1,"models/sprops/misc/alphanum/alphanum_com.mdl")
        }elseif(Text3:explode("")[I2,string]=="-"){
